{
  "name": "ant-design-pro",
  "version": "2.1.1",
  "description": "An out-of-box UI solution for enterprise applications",
  "private": true,
  "scripts": {
    "start": "cross-env APP_TYPE=site umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "dev": "cross-env APP_TYPE=site umi dev",
    "dev:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "node ./scripts/prettier.js"
  },
  "dependencies": {
    "ant-design-pro": "^2.2.1",
    "antd": "^3.15.0",
    "bootstrap-switch": "^3.3.4",
    "braft-editor": "^2.2.9",
    "braft-extensions": "^0.0.18",
    "bundle-loader": "^0.5.6",
    "classnames": "^2.2.6",
    "cropper": "^3.0.0-rc.3",
    "draft-js": "^0.10.5",
    "dva": "^2.4.0",
    "echarts": "^4.1.0",
    "element-react": "^1.4.33",
    "element-theme-default": "^1.4.13",
    "interactjs": "^1.3.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^4.0.0",
    "moment": "^2.22.2",
    "noty": "^3.2.0-beta",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.7.2",
    "qrcode": "^1.3.2",
    "qrcode.react": "^0.9.2",
    "ramda": "^0.26.1",
    "rc-animate": "^2.4.4",
    "react": "^16.6.3",
    "react-bootstrap": "^0.31.5",
    "react-color": "^2.17.0",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.6.3",
    "react-draggable-tab": "^0.10.1",
    "react-hot-loader": "^4.7.2",
    "react-html-parser": "^2.0.2",
    "react-media": "^1.8.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "shortid": "^2.2.14",
    "umi-request": "^1.0.0",
    "video-react": "^0.13.6"
  },
  "devDependencies": {
    "css-loader": "^2.1.1",
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.2",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "tslint": "^5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.6.2",
    "umi-plugin-pro-block": "^1.3.0",
    "umi-plugin-react": "^1.7.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
